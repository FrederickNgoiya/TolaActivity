"\n"
"                                <strong>%(low)s%%</strong> are &gt;%(margin)s"
"%% below target\n"
"                            "
msgstr ""
"\n"
"                        <strong>%(low)s%%</strong> están >%(margin)s%% por "
"debajo del objetivo\n"
"                    "

